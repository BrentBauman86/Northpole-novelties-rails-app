<%= form_for [@category, @toy] do |f| %>
    <div class='form-horizontal'>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <div class='form-horizontal'>
    <%= f.label :quantity %>
    <%= f.text_field :quantity%>
    <br>
    <div class='form-horizontal'>
    <%= f.label :rating %>
    <%= f.text_field :rating%>
    <br>
    <%= f.submit %>
<%end%>
