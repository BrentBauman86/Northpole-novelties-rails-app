<h1>Log In</h1>

<%= form_for @user do |f| %>
     
<%=link_to "Login via Facebook", "/auth/facebook" %>
  
<br></br>
        <div class='form-group'>
          <%= f.label :name %><br />
          <%= f.text_field :name %>
          </div>

          <div class='form-group'>
          <%= f.label :password %><br />
          <%= f.password_field :password%>
          </div>
          
  <%= f.submit "Log In" %>
<%end%>